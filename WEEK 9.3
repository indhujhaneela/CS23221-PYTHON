def abundant(n):
    l=[]
    for i in range(1,n+1):
        for j in range(1,n+1):
            if i*j==n and i!=j:
                l.append(i)
                l.append(j)
    l1=set(l)
    l2=list(l1)
    for i in l2:
        if i==n:
            l2.remove(i)
    if n<sum(l2):
        return 'Yes'
    else:
        return 'No'
